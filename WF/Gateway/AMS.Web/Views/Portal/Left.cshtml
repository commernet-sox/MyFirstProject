                <div class="appButton" title="TMS">
                    <div class="appButton_appIcon">
                        <a href="javascript:void(0);" onclick="subSystemClick('TMS')">
                            <img alt="TMS" src="~/icons/lTMS.png" class="appButton_appIconImg">
                        </a>
                    </div>
                    <div class="appButton_appName">
                        <div class="appButton_appName_inner">TMS</div>
                        <div class="appButton_appName_inner_right"></div>
                    </div>
                </div>
                <div class="dock_tool_list" id="dockToolList">
                    <div class="dock_tool_item" style="        float: none;
        height: 40px;
        width: 40px;
        position: relative;
        cursor: pointer;
        margin: 70px auto 0;" id="fullScreen">
                        <img src="../images/fullscreen.jpg" width="40" height="40" id="imgFullScreen" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script>

        $(document).ready(function () {
            console.log("left...");
            console.log("url1=" + window.parent.location.href);
            console.log("url2=" + window.parent.location.search.substring(1));
            var query = window.parent.location.search.substring(1);
            var vars = query.split("&");
            var redirect = "";
            for (var i = 0; i < vars.length; i++) {
                var pair = vars[i].split("=");
                if (pair[0] == "redirect") { redirect = pair[1]; }
            }
            //window.parent.frames["main"].location.href = "http://www.baidu.com";
            console.log("redirect=" + redirect);
            if (redirect != "") {
                console.log("跳转中。。。");
                window.parent.frames["main"].location.href = decodeURIComponent(redirect);
            }
        });
            
        
        
    </script>
</body>

</html>
