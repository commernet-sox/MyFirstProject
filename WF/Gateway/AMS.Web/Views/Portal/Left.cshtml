
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="utf-8">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="description" content="">
    <meta name="author" content="">

    <title>@ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <style>
        .appListContainer {
            overflow-y: hidden;
            position: relative;
        }

        .appListContainer, .windowContainer {
            left: 0;
            min-height: 100px;
            min-width: 100px;
            overflow-y: auto;
            top: 0;
            width: 100%;
        }

        .appButton:hover, .appButtonEditState .appButton, .appButtonEditState .addQuickLinkButton:hover {
            background: url(../images/appbutton_mouseover_bg3.png) no-repeat scroll 1px 0 transparent;
        }

        .appListContainer .appButton {
            position: absolute;
        }

        .appButton {
            color: white;
            cursor: pointer;
            display: inline;
            float: left;
            font-size: 12px;
            height: 88px;
            position: relative;
            width: 88px;
        }

        .appListContainer .appButton_appIcon {
            margin-top: 6px;
        }

        .appListContainer .appButton_appName {
            overflow: visible;
        }

        .appButton_appName_inner_right {
            display: none;
        }

        .appButton_appIcon {
            background-position: center center;
            background-repeat: no-repeat;
            cursor: pointer;
            height: 48px;
            margin: 0 auto;
            overflow: hidden;
            position: relative;
            width: 48px;
            border-radius:10px;
        }

        .appButton_appIconImg {
            background-position: center center;
            background-repeat: no-repeat;
            height: 100%;
            overflow: hidden;
            position: relative;
            width: 100%;
        }

        img {
            border: none;
        }

        .appButton_appName_table_left {
            display: inline-block;
            height: 20px;
            line-height: 18px;
            max-width: 78px;
            overflow: hidden;
            padding: 0;
            position: relative;
            text-indent: 10px;
            vertical-align: middle;
            width: auto;
        }

        .appButton_appName_table_right {
            width: 10px;
            display: inline-block;
            height: 20px;
            line-height: 18px;
            max-width: 78px;
            overflow: hidden;
            padding: 0;
            position: relative;
            text-indent: 10px;
            vertical-align: middle;
        }

        #leftBar {
            height: 100%;
            left: 0;
            position: absolute;
            top: -12px;
            width: 0;
        }

        .dock_container {
            height: 576px;
            left: 0;
            margin: -290px 0 0;
            position: absolute;
            top: 50%;
            width: 73px;
        }

            .dock_container .appButton_appIcon, .aMg_dock_container .appButton_appIcon {
                margin: 5px auto 0;
            }

        /*.dock_pos_left .dock_middle {
            background: url(../images/dock_l.png ) no-repeat scroll 0 0 transparent;
        }*/

        .dock_middle {
            height: 566px;
            padding-top: 10px;
        }

        .dock_container .appButton {
            height: 58px;
            margin: 2px 3px 2px 4px;
            padding: 0;
            width: 58px;
        }

        .dock_container .appButton_appName {
            display: none;
        }

        .appButton_appName {
            color: white;
            cursor: pointer;
            height: 20px;
            margin: 8px 0 0 0;
            overflow: hidden;
            text-align: center;
        }

        .appButton_appName_inner {
            margin: auto;
            overflow: hidden;
            width: 50px;
            -webkit-user-select: none;
            word-break: break-all;
            display: inline-block;
        }

        .appButton_appName_inner_right {
            display: none;
        }

        .dock_pos_left .dock_tool_list {
            left: 0;
        }

        .dock_tool_list {
            bottom: 5px;
            height: 115px;
            position: absolute;
            width: 65px;
        }

        .dock_tool_item {
            height: 31px;
            margin: 0 auto;
            width: 50px;
        }
        
    </style>
    <script src="~/js/jquery.min.js"></script>
    <script src="~/busjs/http.js" asp-append-version="true"></script>
    <script src="~/busjs/left.js" asp-append-version="true"></script>
</head>
<body style="background-color: #1470cc">
    <div id="leftBar">
        <div id="dockContainer" class="dock_container  dock_pos_left" style="z-index: 10;">
            <div class="dock_middle ui-sortable" style=" background: rgba(0,0,0,0.1);">
